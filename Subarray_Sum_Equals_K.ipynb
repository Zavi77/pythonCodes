      "source": [
        "def fun(nums,k):\n",
        "  res = 0\n",
        "  dc = {0:1}\n",
        "  sum = 0\n",
        "  for i in nums:\n",
        "    sum+=i\n",
        "    diff = sum-k\n",
        "    res +=  dc.get(diff,0)\n",
        "    dc[sum]= 1+dc.get(sum,0)\n",
        "  return res\n",
        "  \n"
      ],
      "execution_count": 64,
      "outputs": []
