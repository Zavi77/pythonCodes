      "source": [
        "class Solution:\n",
        "    def maxProfit(self, prices: List[int]) -> int:\n",
        "        profit = 0\n",
        "        for i in range(1,len(prices)):\n",
        "            profit+=max(0,(prices[i] - prices[i-1]))\n",
        "        return profit"
      ],
