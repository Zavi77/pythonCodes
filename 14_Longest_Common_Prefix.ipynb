      "source": [
        "class Solution:\n",
        "    def longestCommonPrefix(self, strs: List[str]) -> str:\n",
        "        if not strs: return ''\n",
        "        s1 = min(strs)\n",
        "        s2 = max(strs)\n",
        "        \n",
        "        for i,v in enumerate(s1):\n",
        "            if s2[i]!=v:\n",
        "                return s2[:i]\n",
        "        return s1"
      ],
