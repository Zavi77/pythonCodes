      "source": [
        "import math\n",
        "class Solution:\n",
        "    def isPowerOfThree(self, n: int) -> bool:\n",
        "        if n==0:\n",
        "            return False\n",
        "        return n>0 and (math.log10(n)/math.log10(3))%1 == 0"
      ],
